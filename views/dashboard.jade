extends ./layout.jade


block scripts
  // JavaScript
  script(src='js/dashboard.js')
  script(src='js/lodash.min.js')
  script(src='js/backbone-min.js')
  
  

block styles
  // CSS
  link(href='https://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
  link(href='styles/materialize.css', type='text/css', rel='stylesheet', media='screen,projection')
  link(href='styles/style.css', type='text/css', rel='stylesheet', media='screen,projection')
  link(rel='stylesheet', type='text/css', href='styles/joint.min.css')
  link(rel='stylesheet', type='text/css', href='styles/diagram.css')

block content
  .navbar-fixed
    nav.lime.lighten-2.z-depth-3
      .nav-wrapper
        a#logo-container.brand-logo.roboto-bold(href='#') DawChart
        ul.right.hide-on-med-and-down
          // Modal trigger Login - Sign Up
          li
            a.dropdown-button(data-activates='edit', href='#')
              i.material-icons view_module
          li
            a.dropdown-button(data-activates='help', href='#')
              i.material-icons live_help
          li
            a.dropdown-button(data-activates='profile', href='#')
              i.material-icons perm_identity
        ul#nav-mobile.side-nav
          li
            a(href='#') Log in
          li
            a(href='#') Sign Up
        a.button-collapse(href='#', data-activates='nav-mobile')
          i.material-icons menu
    // Diagramas container
    #dashboardContainer
      br 
      br
      br
    
      each item in diagram
        li
          a.btn(id=item.name, onclick='load(this.id)')= item.name
    
          
        
    
